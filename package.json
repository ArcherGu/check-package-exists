{
  "name": "check-package-exists",
  "version": "0.0.0-development",
  "packageManager": "pnpm@10.6.5",
  "description": "Check if the npm package exists",
  "author": "ArcherGu",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/ArcherGu/check-package-exists.git"
  },
  "keywords": [
    "npm",
    "package",
    "exists"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@esm2cjs/execa": "6.1.1-cjs.1",
    "global-dirs": "^4.0.0",
    "resolve": "^1.22.10"
  },
  "devDependencies": {
    "@lightwing/eslint-config": "1.0.111",
    "@types/node": "22.13.10",
    "@types/resolve": "1.20.6",
    "eslint": "9.22.0",
    "lint-staged": "15.5.0",
    "simple-git-hooks": "2.11.1",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md,json,yml}": [
      "eslint --fix"
    ]
  }
}
